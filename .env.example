#######################################
### ENVIRONNEMENT SYMFONY
#######################################

# Environnement de l'application : dev, test, prod
APP_ENV=dev

# Clé secrète utilisée par Symfony (sessions, hash, etc.)
APP_SECRET=ChangeThisToARealSecretKey


#######################################
### BASE DE DONNÉES
#######################################

# Connexion à la base PostgreSQL
# Remplacez "your_database_name" par le nom de votre base
DATABASE_URL="postgresql://postgres:your_password@127.0.0.1:5432/your_database_name?serverVersion=17&charset=utf8"

# ⚠️ Assurez-vous que votre base de données PostgreSQL tourne sur le bon port (5433 ici)


#######################################
### MESSENGER (file d'attente de messages)
#######################################

# Transport utilisé pour la queue
MESSENGER_TRANSPORT_DSN=doctrine://default?auto_setup=0

# Autres options possibles :
# MESSENGER_TRANSPORT_DSN=amqp://guest:guest@localhost:5672/%2f/messages
# MESSENGER_TRANSPORT_DSN=redis://localhost:6379/messages


#######################################
### MAILER (ENVOI D'EMAIL)
#######################################

# DSN du service mail (par défaut désactivé)
MAILER_DSN=null://null

# Exemple pour Mailtrap :
# MAILER_DSN=smtp://username:password@smtp.mailtrap.io:2525


#######################################
### JWT (LexikJWTAuthenticationBundle)
#######################################

# Chemins vers les clés privées/public utilisées pour le JWT
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem

# Phrase secrète utilisée lors de la génération des clés
JWT_PASSPHRASE=your_jwt_passphrase

 

 